<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

	<johnson.fileCopyMonitor>
		<monitors>
			<add xdt:Transform="Insert"
				name="Nintex ImageNow Processor" interval="15" destination="\\jgsm-app-r1\ImageNow\Drop"
			>
				<execute executable="\\jgsm-sql01\Icod.Wod\Icod.Wod.Client.exe" arguments="\\jgsm-app-r1\ImageNow\FileCopy.xml"
					workingDirectory="\\jgsm-sql01\Icod.Wod"/>
				<paths>
					<clear/>
					<add path="\\jcb-imagenow.business.cornell.edu\imagenow\home\ImageNow\NintexPrintService">
						<clear/>
						<add filter="*.Adm - Application ESRG.A-*.pdf"/>
					</add>
				</paths>
			</add>
		</monitors>
	</johnson.fileCopyMonitor>

</configuration>